{
  "name": "@meeshy/gateway",
  "version": "1.0.40-alpha",
  "description": "Fastify Gateway service for Meeshy real-time messaging",
  "private": true,
  "main": "dist/src/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r dotenv/config dist/src/server.js",
    "dev": "tsx watch -r dotenv/config src/server.ts",
    "generate": "prisma generate --schema=../../packages/shared/prisma/schema.prisma",
    "migrate": "prisma migrate dev --schema=../../packages/shared/prisma/schema.prisma",
    "test": "jest --config=jest.config.json",
    "test:watch": "jest --config=jest.config.json --watch",
    "test:coverage": "jest --config=jest.config.json --coverage",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules coverage"
  },
  "dependencies": {
    "@fastify/cors": "^10.1.0",
    "@fastify/helmet": "^12.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/multipart": "^9.3.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.4",
    "@fastify/static": "^8.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.4",
    "@fastify/websocket": "^11.2.0",
    "@grpc/grpc-js": "^1.14.3",
    "@grpc/proto-loader": "^0.7.15",
    "@meeshy/shared": "workspace:*",
    "@prisma/client": "6.13.0",
    "@signalapp/libsignal-client": "^0.86.10",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "firebase-admin": "^13.6.0",
    "fluent-ffmpeg": "^2.1.3",
    "ioredis": "^5.9.1",
    "isomorphic-dompurify": "^2.35.0",
    "jsonwebtoken": "^9.0.3",
    "libphonenumber-js": "^1.12.34",
    "music-metadata": "7.14.0",
    "nice-grpc": "^2.1.14",
    "pdf-parse": "^2.4.5",
    "pino": "^9.14.0",
    "pino-pretty": "^11.3.0",
    "protobufjs": "^7.5.4",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.3",
    "speakeasy": "^2.0.0",
    "ua-parser-js": "^2.0.7",
    "uuid": "^11.1.0",
    "winston": "^3.19.0",
    "zeromq": "^6.5.0",
    "zod": "^3.25.76",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.28",
    "@types/sharp": "^0.32.0",
    "@types/speakeasy": "^2.0.10",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@types/zxcvbn": "^4.4.5",
    "glob": "^11.1.0",
    "grpc-tools": "^1.13.1",
    "jest": "^30.2.0",
    "mongodb": "^6.21.0",
    "prisma": "6.13.0",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
