{
  "name": "@meeshy/shared",
  "version": "1.0.1",
  "description": "Shared library for Meeshy - Types, Prisma Schema, Utilities",
  "private": true,
  "type": "module",
  "main": "./dist/types/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js",
      "default": "./dist/types/index.js"
    },
    "./types/preferences": {
      "types": "./dist/types/preferences/index.d.ts",
      "import": "./dist/types/preferences/index.js",
      "require": "./dist/types/preferences/index.js",
      "default": "./dist/types/preferences/index.js"
    },
    "./types/preferences/*": {
      "types": "./dist/types/preferences/*.d.ts",
      "import": "./dist/types/preferences/*.js",
      "require": "./dist/types/preferences/*.js",
      "default": "./dist/types/preferences/*.js"
    },
    "./types/validation": {
      "types": "./dist/types/validation/index.d.ts",
      "import": "./dist/types/validation/index.js",
      "require": "./dist/types/validation/index.js",
      "default": "./dist/types/validation/index.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/types/*.js",
      "require": "./dist/types/*.js",
      "default": "./dist/types/*.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js",
      "require": "./dist/utils/*.js",
      "default": "./dist/utils/*.js"
    },
    "./encryption": {
      "types": "./dist/encryption/index.d.ts",
      "import": "./dist/encryption/index.js",
      "require": "./dist/encryption/index.js",
      "default": "./dist/encryption/index.js"
    },
    "./encryption/*": {
      "types": "./dist/encryption/*.d.ts",
      "import": "./dist/encryption/*.js",
      "require": "./dist/encryption/*.js",
      "default": "./dist/encryption/*.js"
    },
    "./prisma": {
      "types": "./prisma/schema.prisma"
    },
    "./prisma/client": {
      "types": "./prisma/client/index.d.ts",
      "import": "./prisma/client/index.js",
      "require": "./prisma/client/index.js",
      "default": "./prisma/client/index.js"
    }
  },
  "files": [
    "dist",
    "prisma",
    "types",
    "utils",
    "encryption"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build:watch": "tsc --watch --project tsconfig.json",
    "generate": "prisma generate --schema=./prisma/schema.prisma --generator client",
    "generate:all": "prisma generate --schema=./prisma/schema.prisma",
    "generate:python": "prisma generate --schema=./prisma/schema.prisma --generator client_py",
    "clean": "rm -rf dist node_modules",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prisma:studio": "prisma studio --schema=./prisma/schema.prisma",
    "prisma:format": "prisma format --schema=./prisma/schema.prisma"
  },
  "keywords": [
    "meeshy",
    "types",
    "typescript",
    "shared",
    "library",
    "prisma"
  ],
  "author": "Meeshy Team",
  "license": "UNLICENSED",
  "dependencies": {
    "@prisma/client": "^7.3.0",
    "zod": "^3.25.76"
  },
  "optionalDependencies": {
    "@signalapp/libsignal-client": "^0.58.3"
  },
  "devDependencies": {
    "@types/node": "^22.19.7",
    "@vitest/coverage-v8": "^3.2.4",
    "prisma": "^6.15.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma"
  }
}
